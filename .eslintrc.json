{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      2,
      2
    ],
    "no-unused-vars": 2,
    "no-console": 0,
    "semi": [
      "error",
      "always"
    ]
  }
}